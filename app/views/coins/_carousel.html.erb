<div class="col s12 m12 l12">
  <div class="carousel carousel-slider center" data-indicators="true">
    <!-- <div class="carousel-fixed-item center">
      <a class="btn waves-effect blue white-text darken-text-2">button</a>
    </div> -->
    <% count = 0 %>
    <% @top_5_coins.each do |coin| %>
    <% ext = @cc_coin_list[coin.symbol] %>
      <% #if @top_5_cc_symbol[count] == "GBG"  %>
        <% #@top_5_cc_symbol[count] = "GOLOS" %>
      <div class="carousel-item black-text" href="<%= @carousel_position[count] %>">
        <div class="row">
          <div class="col s5 coin-logo" id="carousel-item<%= count %>">
            <img src="<%= ext[:image_url] %>" class="carousel-img">
          </div>
          <div class="col s7 text-left">
            <h5>Biggest % Gain</h5>
            <h3><%= link_to "#{ ext[:full_name] }", "coins/#{coin.symbol}" %></h3>
            <h5> <%= coin.pct_change %>%</h5>
            <h5>Last Price <%= coin.last_price %></h5>
            <h5>24hr Volume <%= coin.volume %> BTC</h5>
            <% count += 1 %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
