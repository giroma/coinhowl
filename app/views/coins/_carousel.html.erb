<div class="col s12 m12 l12">
  <div class="carousel carousel-slider center" data-indicators="true">
    <!-- <div class="carousel-fixed-item center">
      <a class="btn waves-effect blue white-text darken-text-2">button</a>
    </div> -->
    <% count = 0 %>
    <% @top_5_coins.each do |coin| %>
      <div class="carousel-item black-text" href="<%= @carousel_position[count] %>">
        <div class="row">
          <div class="col s5 coin-logo" id="carousel-item<%= count %>">
          <img src="https://www.cryptocompare.com<%= @cryptocompare_result[@top_5_cc_symbol[count]]["ImageUrl"] %>">
          </div>
          <div class="col s7 text-left">
            <h5>Biggest % Gain</h5>
            <h3><%= link_to "#{ coin[1][:original]['MarketName'] }", "coins/#{@top_5_cc_symbol[count]}" %></h3>
            <h5> <%= number_with_precision(coin[1][:change], precision: 2) %>%</h5>
            <h5>Last Price <%= number_with_precision(coin[1][:original]["Last"], precision: 8) %></h5>
            <h5>24hr Volume <%= number_with_precision(coin[1][:original]["BaseVolume"], precision: 2) %> BTC</h5>
              <% count+=1 %>
            </div>
          </div>
      </div>
    <% end %>
  </div>
</div>
